{
  "name": "TIA-izip",
  "version": "1.0.0",
  "engines": {
    "node": "7.4.0"
  },
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "build": "run-s clean build:server build:common build:app",
    "build:app": "webpack --config webpack.babel.js --progress --colors",
    "build:server": "babel ./src/backend --out-dir dist/backend",
    "build:common": "babel ./src/common --out-dir dist/common",
    "clean": "run-s clean:dist clean:bundles",
    "clean:dist": "rm -rf dist/*",
    "clean:bundles:": "rm -rf public/bundles/*",
    "dev": "run-s clean:bundles dev:server",
    "dev:server": "babel-watch ./src/backend/server.js",
    "heroku-postbuild": "NODE_ENV=production run-s build run:migrations",
    "lint": "./node_modules/.bin/eslint .",
    "sort": "npm-sort",
    "start": "NODE_ENV=production node ./dist/backend/server.js",
    "test": "npm run lint",
    "resetdb": "run-s db:recreate run:migrations run:seeds",
    "db:recreate": "babel-node scripts/resetdb.js",
    "run:migrations": "knex migrate:latest --knexfile ./src/backend/knex/knexfile.js",
    "run:seeds": "knex seed:run --knexfile ./src/backend/knex/knexfile.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.23.0",
    "babel-loader": "^6.3.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "babel-watch": "^2.0.6",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.1",
    "bootstrap": "^3.3.7",
    "crypto": "0.0.3",
    "css-loader": "^0.26.1",
    "dotenv": "^4.0.0",
    "eslint": "^3.15.0",
    "express": "^4.14.1",
    "express-session": "^1.15.2",
    "file-loader": "^0.10.0",
    "json-loader": "^0.5.4",
    "knex": "^0.12.9",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "nodemailer": "^3.1.8",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.1",
    "pg": "^6.1.5",
    "postcss-loader": "^1.3.1",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.8",
    "react-datetime": "^2.8.8",
    "react-dom": "^15.4.2",
    "react-notification-system": "^0.2.13",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-form": "^6.6.0",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "validator": "^7.0.0",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "npm-sort": "0.0.4",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1"
  }
}
