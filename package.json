{
  "name": "TIA-izip",
  "version": "1.0.0",
  "engines": {
    "node": "7.4.0"
  },
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "build": "run-s clean build:server build:app",
    "build:app": "webpack --config webpack.babel.js --progress --colors",
    "build:server": "babel ./src/common/* ./src/server/* --out-dir dist",
    "clean": "run-s clean:dist clean:bundles",
    "clean:dist": "rm -rf dist/*",
    "clean:bundles:": "rm -rf public/bundles/*",
    "dev": "run-s clean:bundles dev:server",
    "dev:server": "babel-watch ./src/server/server.js",
    "lint": "./node_modules/.bin/eslint .",
    "sort": "npm-sort",
    "start": "NODE_ENV=production node ./dist/src/server/server.js",
    "test": "npm run lint"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.23.0",
    "babel-loader": "^6.3.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.23.0",
    "babel-watch": "^2.0.6",
    "css-loader": "^0.26.1",
    "dotenv": "^4.0.0",
    "eslint": "^3.15.0",
    "express": "^4.14.1",
    "file-loader": "^0.10.0",
    "json-loader": "^0.5.4",
    "npm-run-all": "^4.0.1",
    "postcss-loader": "^1.3.1",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.8",
    "react-dom": "^15.4.2",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "npm-sort": "0.0.4",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1"
  }
}
